<main role="main">
<div class="container">

  <div class="page-header header">
    <h2>寻找餐厅</h2>
  </div>

  <div class="row">
    <div class="col-md-6 col-sm-6">
      <%= render 'shared/search' %>
    </div>
  </div>
</div>

<div class="container stores">
  <div class="row">
    <div class="stores-grid col-sm-9">
      <%= render @stores %>
    </div>

    <div class="cart-main col-sm-3">
      <%= render 'carts/carts', cart: @cart %>
    </div>
  </div>
</div>
</main>
